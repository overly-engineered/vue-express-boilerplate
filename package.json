{
  "name": "chatter",
  "author": "Jamie Pettman",
  "description": "Simple instant messenger app",
  "license": "ISC",
  "version": "0.1.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "rm -rf ./dist && webpack --mode=production",
    "build:watch": "webpack --watch --mode=development",
    "lint": "tslint --project . -c tslint.json --fix",
    "prebuild": "npm run lint",
    "server": "nodemon --delay 1000ms --verbose --watch dist ./dist/server.js",
    "start:dev": "npm-run-all -p build:watch server",
    "start": "node ./dist/server.js",
    "test": "echo No tests yet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint"
    }
  },
  "devDependencies": {
    "@types/express": "4.17.3",
    "@types/express-session": "1.17.0",
    "@types/node": "12.12.31",
    "css-loader": "6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.2.3",
    "nodemon": "2.0.2",
    "random-words": "1.1.0",
    "ts-loader": "9.4.2",
    "tslint": "6.1.0",
    "typescript": "3.7.5",
    "vue": "2.6.11",
    "vue-loader": "17.0.1",
    "vue-property-decorator": "8.4.0",
    "vue-template-compiler": "2.6.11",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-session": "1.17.0",
    "http": "0.0.1-security",
    "lodash": "4.17.19",
    "npm-run-all": "4.1.5",
    "webpack-node-externals": "1.7.2"
  }
}
